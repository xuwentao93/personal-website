{
  "name": "personal-blog",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "testPromise": "promises-aplus-tests src/myPromise.js",
    "build": "vite build --config config/vite.config.ts",
    "build:ssr": "npm run build:client && npm run build:server",
    "build:client": "vite build --config config/vite.config.js --ssrManifest --outDir dist/client",
    "build:server": "vite build --config config/vite.config.js --ssr src/server-entry.tsx --outDir dist/server",
    "server": "cross-env NODE_ENV=production node server.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/node": "^17.0.21",
    "@types/spark-md5": "^3.0.4",
    "antd": "^5.9.4",
    "axios": "^0.19.1",
    "express": "^4.18.2",
    "markdown-it": "^12.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.1",
    "react-markdown-editor-lite": "^1.3.2",
    "react-router": "^6.3.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.8.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1",
    "spark-md5": "^3.0.2",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^4.1.1",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "promises-aplus-tests": "^2.1.2",
    "typescript": "^4.5.4",
    "vike": "^0.4.168",
    "vite": "^4.1.0"
  }
}
